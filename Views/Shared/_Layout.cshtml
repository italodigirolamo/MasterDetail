<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Store Master Detail</title>
    @* <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" /> *@
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    
    <link rel="stylesheet" href="~/MasterDetail.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css">
</head>
<body>
    <header>

        <nav class="navbar navbar-expand-lg bg-dark" data-bs-theme="dark">
            <div class="container-fluid">
                <a class="navbar-brand flex-column w-4 link-underline" href="Home">Store Master Detail</a>

                @* <img src="/docs/5.3/assets/brand/bootstrap-logo.svg" alt="Bootstrap" width="30" height="24"> *@
                <img src="~/admin/img/logo.svg" width="60" />


                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarColor02" aria-controls="navbarColor02" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarColor02">
                    <ul class="navbar-nav me-auto">
                        <li class="nav-item">
                            <a class="nav-link active" href="Home">
                                Home
                                <span class="visually-hidden">(current)</span>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-controller="Clientes" asp-action="Index">Clientes</a>
                            @* <a class="nav-link" href="Clientes">Clientes</a> *@
                        </li>
                        <li class="nav-item">
                           

                            <a class="nav-link" asp-controller="Producto" asp-action="Index">Producto</a>
                            @* <a class="nav-link" href="Producto">Producto</a> *@
                        </li>

                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" data-bs-toggle="dropdown" href="" role="button" aria-haspopup="true" aria-expanded="false">Transacciones</a>
                            <div class="dropdown-menu">
                                @* <a class="dropdown-item" href="Pedido">Pedidos</a> *@
                                <a class="dropdown-item" asp-controller= "Pedido">Pedidos</a>
                                <a class="dropdown-item" asp-controller= "Cotizacion">Cotizaciones</a>
                                <a class="dropdown-item" asp-controller= "Consultas">Consultas</a>
                                <div class="dropdown-divider"></div>
                                <a class="dropdown-item" asp-controller= "#">Mantenimiento</a>
                            </div>
                        </li>

                        <li class="nav-item">
                            <a class="nav-link" href="#">About</a>
                        </li>

                    </ul>
                    <form class="d-flex">
                        <input class="form-control me-sm-2" type="search" placeholder="Search">
                        <button class="btn btn-secondary my-2 my-sm-0" type="submit">Search</button>
                    </form>
                </div>
            </div>
        </nav>


    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

     @* navbar navbar-expand-lg bg-dark" data-bs-theme="dark *@
    @* <footer class="border-top footer text-muted"> *@
    @* class="navbar navbar-expand-lg bg-dark" data-bs-theme="dark" *@
        
        @* <footer class="footer navbar-brand navbar-expand-lg bg-dark" data-bs-theme="dark"> *@

    <footer class="footer  navbar-nav me-auto bg-dark" data-bs-theme="dark">
        <div class="container">
            Made by Italo DEV with <i class="bi bi-heart-fill"></i>
            &copy; 2024 - Master Detail - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>

    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
